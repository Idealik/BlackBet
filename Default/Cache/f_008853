    <script>
        var setURLs = function(urls) {
            if ($('#detailExport140').prop('checked')) {
                $('#publicLink140').val(urls['publicRequestDetailedURL']);
                $('#authLink140').val(urls['authRequestDetailedURL']);
            } else {
                $('#publicLink140').val(urls['publicRequestURL']);
                $('#authLink140').val(urls['authRequestURL']);
            }
        };
    </script>


    <script>
        exportPopups["140"] = new ExportIcalInterface(
            0, null, false,
            false, true, setURLs, "/api/build-urls",
            {confId: "140"}, {"authRequestDetailedURL":"","authRequestURL":"","publicRequestDetailedURL":"https://indico-hlit.jinr.ru/export/event/140.ics?detail=events","publicRequestURL":"https://indico-hlit.jinr.ru/export/event/140.ics"}, "140",
            "307"
        );

