        var setURLs = function(urls) {
            if ($('#detailExport455').prop('checked')) {
                $('#publicLink455').val(urls['publicRequestDetailedURL']);
                $('#authLink455').val(urls['authRequestDetailedURL']);
            } else {
                $('#publicLink455').val(urls['publicRequestURL']);
                $('#authLink455').val(urls['authRequestURL']);
            }
        };
    </script>


