    <script>
        var setURLs = function(urls) {
            if ($('#detailExport140').prop('checked')) {
                $('#publicLink140').val(urls['publicRequestDetailedURL']);
                $('#authLink140').val(urls['authRequestDetailedURL']);
            } else {
                $('#publicLink140').val(urls['publicRequestURL']);
                $('#authLink140').val(urls['authRequestURL']);
            }
        };
    </script>


